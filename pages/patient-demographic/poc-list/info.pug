extends ../../../templates/layout

block content



    // page-header
    .page-header
        ol.breadcrumb
            // breadcrumb
            li.breadcrumb-item
                a(href='#') Home
            li.breadcrumb-item.active(aria-current='page') Poc List Summary Assessment Report
        // End breadcrumb
    // End page-header

    .row
        .col-md-12
            .card.card-collapsed
                .card-header
                    h3.card-title PLAN OF CARE ( PROBLEM / INTERVENTION / GOALS / STATUS )
                    .card-options
                        a.card-options-collapse(href='#' data-toggle='card-collapse')
                        a.card-options-remove(href='#' data-toggle='card-remove')
                            i.fe.fe-x
                .card-body
                    .btn-list.text-right
                        a.btn.btn-primary.btn-sm(href='/nursing/assessment-form')
                            span
                                i.fa.fa-plus
                                |  ADD
                    br
                    .table-responsive
                        table.table.card-table.table-vcenter.text-nowrap.table-nowrap
                            thead.bg-primary.text-white
                                tr
                                    th.text-white DISCIPLINE
                                    th.text-white SPECIAL INSTRUCTION

                            tbody
                                tr
                                    th(scope='row')
                                    td.small


